		lineageTag: 95804127-a147-4def-9d86-d10cae2e5230
		summarizeBy: none
		sourceColumn: discharge_description

		annotation SummarizationSetBy = Automatic

	/// Mortality classification flag. Categorizes outcomes as either 'Deaths' or 'Survivors' for mortality analysis and key performance indicators.
	column Mortality
		dataType: string
		lineageTag: 52ab8039-26c1-4ef2-b640-d2d441a3a745
