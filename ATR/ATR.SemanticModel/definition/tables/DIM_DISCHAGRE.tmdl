	/// Discharge destination identifier. Primary key for where patient went after hospital treatment.
	column destination_id
		dataType: int64
		formatString: 0
		lineageTag: fe65b0bb-5c50-4d06-bc29-dee1980b8ee7
		summarizeBy: none
		sourceColumn: destination_id

		annotation SummarizationSetBy = Automatic

